_50.Name = "50"
_50.Parent = main
_50.BackgroundColor3 = Color3.fromRGB(185, 185, 185)
_50.BorderColor3 = Color3.fromRGB(0, 0, 0)
_50.Position = UDim2.new(0, 0, 0.234130397, 0)
_50.Size = UDim2.new(0, 68, 0, 25)
_50.Font = Enum.Font.SourceSans
_50.Text = "50"
_50.TextColor3 = Color3.fromRGB(0, 0, 0)
_50.TextSize = 14.000
_50.MouseButton1Down:connect(function()
	while wait() do
		game:GetService("ReplicatedStorage").Events.CrashAction:FireServer("Bet","50","1.1")

		wait(6)

	end
end)

